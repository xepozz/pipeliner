{
  "name": "xepozz/pipeliner",
  "description": "A sequence runner in PHP",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.4"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Xepozz\\Pipeliner\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Xepozz\\Pipeliner\\Tests\\": "tests/"
    }
  }
}
